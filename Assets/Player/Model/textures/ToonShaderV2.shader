			float3 worldPos;
			float3 worldNormal;
			INTERNAL_DATA
		};

		uniform sampler2D _NormalMap;
		uniform float4 _NormalMap_ST;
		uniform float _DeapthMap;
		uniform sampler2D _BaseTextur;
		uniform float4 _BaseTextur_ST;
		uniform float _Power;

		void surf( Input i , inout SurfaceOutputStandard o )
		{
			float2 uv_NormalMap = i.uv_texcoord * _NormalMap_ST.xy + _NormalMap_ST.zw;
			o.Normal = UnpackScaleNormal( tex2D( _NormalMap, uv_NormalMap ), _DeapthMap );
			float2 uv_BaseTextur = i.uv_texcoord * _BaseTextur_ST.xy + _BaseTextur_ST.zw;
			o.Albedo = tex2D( _BaseTextur, uv_BaseTextur ).rgb;
			float3 ase_worldPos = i.worldPos;
			float3 ase_worldViewDir = normalize( UnityWorldSpaceViewDir( ase_worldPos ) );
			float3 ase_worldNormal = WorldNormalVector( i, float3( 0, 0, 1 ) );
			float fresnelNdotV3 = dot( ase_worldNormal, ase_worldViewDir );
			float fresnelNode3 = ( 0.0 + ( 6.0 * _Power ) * pow( 1.0 - fresnelNdotV3, _Power ) );
			float4 color2 = IsGammaSpace() ? float4(1,1,1,1) : float4(1,1,1,1);
			o.Emission = ( float4( 0,0,0,0 ) - ( fresnelNode3 * color2 ) ).rgb;
			o.Alpha = 1;
		}

		ENDCG
